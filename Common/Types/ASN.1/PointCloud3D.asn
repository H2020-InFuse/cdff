PointCloud3DNS DEFINITIONS ::=
BEGIN

PointCloud3DMode ::= ENUMERATED 
	{
	base-mode,
	with-normals
	}

PointCloud3D ::= SEQUENCE
	{
	point-cloud-time		INTEGER,
	frame-identifier		T61String,
	data				SEQUENCE OF REAL,
	size				INTEGER,
	point-cloud-mode		PointCloud3DMode
	}

END

